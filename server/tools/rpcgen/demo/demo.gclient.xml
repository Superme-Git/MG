<?xml version="1.0" encoding="gbk"?>

<application name="demo" shareHome="../../../../share" pvids="">

	<import file="../../gnet/gnet.xml"/>
	<import file="demo.gmapd.xml"/>
	<import file="demo.gsd.xml"/>

	<state name="Client">
		<protocol ref="gnet.KeepAlive"/>
		<protocol ref="gnet.Challenge"/>
		<protocol ref="gnet.Response"/>
		<provider ref="gs"/>
		<provider ref="mapo"/>
	</state>

	<service name="gclient">
		<manager name="LinkClient" type="client" initstate="Client" port="10000">
			<state ref="Client"/>
		</manager>
	</service>
</application>

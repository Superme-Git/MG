<Window x:Class="ExcelParse2.SelectXlsDlg"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        Title="SelectXlsDlg" Height="566" Width="879"
        Loaded="Window_Loaded">
    <Grid>
        <Label Content="请输入过滤串" HorizontalAlignment="Left" Height="23" Margin="10,10,0,0" VerticalAlignment="Top" Width="281"/>
        <ListBox x:Name="mSrcList" HorizontalAlignment="Left" Height="383" Margin="10,112,0,0" VerticalAlignment="Top" Width="419" MouseDoubleClick="mSrcList_MouseDoubleClick"/>
        <Label Content="目的生成列表" HorizontalAlignment="Left" Height="23" Margin="442,10,0,0" VerticalAlignment="Top" Width="281"/>
        <ListBox x:Name="mDstList" HorizontalAlignment="Left" Height="461" Margin="442,33,0,0" VerticalAlignment="Top" Width="419" MouseDoubleClick="mDstList_MouseDoubleClick"/>
        <Button Content="确定" HorizontalAlignment="Left" Height="26" Margin="668,499,0,0" VerticalAlignment="Top" Width="94" Click="Button_ClickOk"/>
        <Button Content="取消" HorizontalAlignment="Left" Height="26" Margin="767,499,0,0" VerticalAlignment="Top" Width="94" Click="Button_ClickCancel"/>
        <TextBox x:Name="mInputBox" HorizontalAlignment="Left" Height="23" Margin="10,37,0,0" TextWrapping="Wrap" Text="TextBox" VerticalAlignment="Top" Width="419" TextChanged="mInputBox_TextChanged"/>
        <Label Content="源xls列表" HorizontalAlignment="Left" Height="24" Margin="11,88,0,0" VerticalAlignment="Top" Width="68"/>
        <Label Content="双击移动到目的生成列表" HorizontalAlignment="Left" Height="24" Margin="11,501,0,0" VerticalAlignment="Top" Width="372"/>
        <ComboBox x:Name="mSrcCombox" HorizontalAlignment="Left" Margin="309,68,0,0" VerticalAlignment="Top" Width="120" SelectedIndex="0" SelectionChanged="ComboBox_SelectionChanged">
            <ComboBoxItem Content="按名称排序"/>
            <ComboBoxItem Content="按修改时间排序"/>
        </ComboBox>
        <Label Content="排序规则：" HorizontalAlignment="Left" Margin="246,65,0,0" VerticalAlignment="Top"/>
        <CheckBox x:Name="mSrcCheckBox2" Content="复合" HorizontalAlignment="Left" Margin="28,71,0,0" VerticalAlignment="Top" RenderTransformOrigin="-0.263,0.867" Width="51" Checked="CheckBox_Checked_2" IsChecked="True"/>
        <CheckBox x:Name="mSrcCheckBox3" Content="文件夹" HorizontalAlignment="Left" Margin="96,72,0,0" VerticalAlignment="Top" RenderTransformOrigin="-0.175,0.6" Checked="CheckBox_Checked_3"/>

    </Grid>
</Window>

<Window x:Class="ExcelParse.SelectFileDlg"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        Title="SelectFileDlg" Height="777.413" Width="1165.824">
    <Grid Margin="0,0,0,7">
        <Grid.ColumnDefinitions>
            <ColumnDefinition Width="270*"/>
            <ColumnDefinition Width="22*"/>
            <ColumnDefinition Width="287*"/>
        </Grid.ColumnDefinitions>
        <ListBox x:Name="mLeftList" HorizontalAlignment="Left" Height="594" Margin="65,117,0,0" VerticalAlignment="Top" Width="458"/>
        <ListBox x:Name="mRightList" Grid.Column="2" HorizontalAlignment="Left" Height="601" Margin="109,45,0,0" VerticalAlignment="Top" Width="405"/>
        <Button x:Name="mLeftButton" Content="右移>" HorizontalAlignment="Left" Height="22" Margin="29,83,0,0" VerticalAlignment="Top" Width="93" Click="mLeftButton_Click" Grid.Column="1" Grid.ColumnSpan="2"/>
        <Button x:Name="mRightButton" Content="左移" HorizontalAlignment="Left" Height="23" Margin="29,117,0,0" VerticalAlignment="Top" Width="93" RenderTransformOrigin="0.412,0.696" Click="mRightButton_Click" Grid.Column="1" Grid.ColumnSpan="2"/>
        <Button x:Name="mOkButton" Content="确定" Grid.Column="2" HorizontalAlignment="Left" Height="30" Margin="285,681,0,0" VerticalAlignment="Top" Width="91" Click="mOkButton_Click"/>
        <Button x:Name="mCancelButton" Content="取消" Grid.Column="2" HorizontalAlignment="Left" Height="30" Margin="425,681,0,0" VerticalAlignment="Top" Width="89" Click="mCancelButton_Click" RenderTransformOrigin="0.787,2.299"/>
        <Label Content="源文件列表" HorizontalAlignment="Left" Height="30" Margin="65,82,0,0" VerticalAlignment="Top" Width="235"/>
        <Label Content="目标文件列表" HorizontalAlignment="Left" Height="30" Margin="78,10,0,0" VerticalAlignment="Top" Width="228" Grid.Column="2"/>
        <TextBox HorizontalAlignment="Left" Height="23" Margin="65,59,0,0" TextWrapping="Wrap" VerticalAlignment="Top" Width="458" TextChanged="TextBox_TextChanged"/>
        <Label Content="搜索字符串" HorizontalAlignment="Left" Height="29" Margin="65,24,0,0" VerticalAlignment="Top" Width="327"/>
    </Grid>
</Window>

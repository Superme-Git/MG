<Window x:Name="mProgressBarWindow" x:Class="ExcelParse.ProgressBarWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        Title="ProgressBarWindow" Height="371" Width="539" HorizontalAlignment="Center" Topmost="True" ResizeMode="NoResize">
    <Grid Margin="0,0,0,1">
        <Grid.ColumnDefinitions>
            <ColumnDefinition Width="113*"/>
            <ColumnDefinition Width="44*"/>
            <ColumnDefinition Width="388*"/>
        </Grid.ColumnDefinitions>
        <Label x:Name="mTaskType" Grid.ColumnSpan="3" Content="Label" HorizontalAlignment="Left" Height="28" Margin="31,10,0,0" VerticalAlignment="Top" Width="467"/>
        <ProgressBar x:Name="mTaskProgress" Grid.ColumnSpan="3" HorizontalAlignment="Left" Height="22" Margin="31,43,0,0" VerticalAlignment="Top" Width="467"/>
        <Label x:Name="mOutputWindowType" Grid.ColumnSpan="3" Content="输出信息" HorizontalAlignment="Left" Height="26" Margin="31,70,0,0" VerticalAlignment="Top" Width="467"/>
        <ListBox x:Name="mLogWindow" Grid.ColumnSpan="3" HorizontalAlignment="Left" Height="220" Margin="31,101,0,0" VerticalAlignment="Top" Width="467"/>

    </Grid>
</Window>

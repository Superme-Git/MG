<?xml version="1.0" encoding="gb2312"?>
<VisualStudioPropertySheet
	ProjectType="Visual C++"
	Version="8.00"
	Name="gxwaddin"
	>
	<Tool
		Name="VCCLCompilerTool"
		AdditionalIncludeDirectories="$(GxwDir);$(AddinDir)"
	/>
	<UserMacro
		Name="GxwDir"
		Value="../"
	/>
	<UserMacro
		Name="AddinDir"
		Value="$(GxwDir)/include/addin/"
	/>
	<UserMacro
		Name="NuclearDir"
		Value="..\.."
	/>
</VisualStudioPropertySheet>

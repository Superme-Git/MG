<Window x:Class="QuestInput.InsertOpenDialog"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        Title="InsertOpenDialog" Height="245" Width="352">
    <Grid Margin="10,0,50,29">
        <Label Content="输入要显示的文字(颜色是主界面选择颜色)" HorizontalAlignment="Left" Margin="10,10,0,0" VerticalAlignment="Top" Width="233"/>
        <TextBox x:Name="IO_tbTextShow" HorizontalAlignment="Left" Height="23" Margin="10,40,0,0" TextWrapping="Wrap" Text="" VerticalAlignment="Top" Width="249"/>
        <ComboBox x:Name="IO_cbChooseDD" HorizontalAlignment="Left" Margin="10,98,0,0" VerticalAlignment="Top" Width="233" Height="25"></ComboBox>
        <Label Content="选择打开的界面" HorizontalAlignment="Left" Margin="10,68,0,0" VerticalAlignment="Top" Width="106"/>
        <Button Content="确定" HorizontalAlignment="Left" Margin="93,150,0,0" VerticalAlignment="Top" Width="75" Height="25" Click="HandleInsertOpendialogSureClick"/>
    </Grid>
</Window>

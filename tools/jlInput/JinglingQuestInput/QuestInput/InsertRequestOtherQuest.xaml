<Window x:Class="QuestInput.InsertRequestOtherQuest"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        Title="InsertRequestOtherQuest" Height="300" Width="300">
    <Grid>
        <Label Content="输入想要显示的文字（颜色同主界面设置颜色）" HorizontalAlignment="Left" Margin="10,20,0,0" VerticalAlignment="Top" Width="262"/>
        <TextBox x:Name="IR_tbShowText" HorizontalAlignment="Left" Height="23" Margin="10,50,0,0" TextWrapping="Wrap" Text="" VerticalAlignment="Top" Width="262" RenderTransformOrigin="0.208,0"/>
        <Label Content="输入问题ID" HorizontalAlignment="Left" Margin="10,102,0,0" VerticalAlignment="Top" Width="118" RenderTransformOrigin="0.534,-0.04"/>
        <TextBox x:Name="IR_tbQuestID" HorizontalAlignment="Left" Height="23" Margin="10,132,0,0" TextWrapping="Wrap" Text="" VerticalAlignment="Top" Width="128"/>
        <Button Content="确定" HorizontalAlignment="Left" Margin="98,215,0,0" VerticalAlignment="Top" Width="75" Height="26" Click="handleRequestOtherQuestSureClick"/>

    </Grid>
</Window>
